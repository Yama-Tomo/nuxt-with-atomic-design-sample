  },
  "devDependencies": {
    "@nuxt/typescript": "2.8.1",
    "@nuxtjs/eslint-config": "0.0.1",
    "@types/hard-source-webpack-plugin": "1.0.1",
    "@types/lodash": "4.14.134",
    "@types/node": "12.0.7",
    "@types/webpack": "4.4.32",
